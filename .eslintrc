{ "plugins": ["cypress"],
  "extends": ["plugin:cypress/recommended", "react-app", "react-app/jest", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": ["error",
      {
        "singleQuote": true,
        "printWidth": 100,
        "semi": true,
        "jsxSingleQuote": true,
        "arrowParens": "avoid"
  }]},
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "cypress/globals": true
  },
  "parser": "babel-eslint",
  "globals": {
    "cy": true
  }
}